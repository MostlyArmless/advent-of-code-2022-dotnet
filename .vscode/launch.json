  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Program.cs",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/AdventOfCode/bin/Debug/net7.0/AdventOfCode.dll",
      "args": [
        "1"
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
